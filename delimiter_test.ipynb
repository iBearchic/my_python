   "source": [
    "def delimit(expr):\n",
    "    di={\"{\": 0,\"}\": 0,\"[\": 0,\"]\": 0,\"(\": 0,\")\": 0}\n",
    "    s=expr.split()\n",
    "    \n",
    "    for i in range(len(s)):\n",
    "        di[s[i]]+=1\n",
    "    \n",
    "    m=list(di.values())\n",
    "    return m[0]==m[1] and m[2]==m[3] and m[4]==m[5]\n",
    "    "
   ]
  },
